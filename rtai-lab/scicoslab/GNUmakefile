$(srcdir)/GNUmakefile.in: # $(srcdir)/GNUmakefile.am  $(am__configure_deps)
	@for dep in $?; do \
	  case '$(am__configure_deps)' in \
	    *$$dep*) \
	      cd $(top_builddir) && $(MAKE) $(AM_MAKEFLAGS) am--refresh \
		&& exit 0; \
	      exit 1;; \
	  esac; \
	done; \
	echo ' cd $(top_srcdir) && $(AUTOMAKE) --foreign  rtai-lab/scicoslab/GNUmakefile'; \
	cd $(top_srcdir) && \
	  $(AUTOMAKE) --foreign  rtai-lab/scicoslab/GNUmakefile

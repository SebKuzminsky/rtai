install-dbx-htmldocs: $(DBX_ALL_HTML:%=$(HTML_DOCSDIR)%)
	docs="$^"; \
	for dir in $$docs; do \
	    dest=$(DESTDIR)$(dbx_docdir)/html/`basename $$dir`; \
	    $(mkinstalldirs) $$dest; \
	    abs_dest=`cd $$dest && pwd` ; \
            pushd $$dir; \
	    for f in * ; do \
	        case $$f in \
		    *~|CVS|[mM]akefile*|GNUmakefile*);; \
		    *.html|*.htm) $(DBX_HTML_TRANSFORM) $$f > $$abs_dest/$$f;; \
		    *) $(INSTALL_DATA) $$f $$abs_dest/$$f;; \
	        esac; \
	    done; \
	    popd; \
	done


base/GNUmakefile: @RTAI_LINUX_DIR@/.config
	@echo "*****************************************" ; \
	 echo "*  The Linux configuration has changed  *" ; \
	 echo "*  forcing 'make reconfig' ...          *" ; \
	 echo "*****************************************" ; \
	$(MAKE) reconfig

clean-if-reconfigured:
	@if test -r .cfchanged ; then \
	   rm -f .cfchanged ; \
	   echo "****************************************" ; \
	   echo "*  Your RTAI configuration has changed *" ; \
	   echo "*  forcing 'make clean' ...            *" ; \
	   echo "****************************************" ; \
	   $(MAKE) clean ; \
	fi

reconfig xconfig gconfig mconfig menuconfig config oldconfig:
	@$(MAKE) -f $(srcdir)/makefile $@ \
	srctree=$(srcdir) ARCH=@RTAI_HOST_STRING@ CROSS_COMPILE=@CROSS_COMPILE@

@CONFIG_RTAI_OLD_FASHIONED_BUILD_TRUE@clean-local:
@CONFIG_RTAI_OLD_FASHIONED_BUILD_TRUE@	rm -fr modules .cfchanged

@CONFIG_RTAI_OLD_FASHIONED_BUILD_TRUE@distclean-local:
@CONFIG_RTAI_OLD_FASHIONED_BUILD_TRUE@	$(MAKE) -C $(srcdir)/base/config/kconfig \
@CONFIG_RTAI_OLD_FASHIONED_BUILD_TRUE@	-f Makefile.kconfig distclean srctree=$(srcdir)
@CONFIG_RTAI_OLD_FASHIONED_BUILD_TRUE@	find . -name autom4te.cache | xargs rm -fr
@CONFIG_RTAI_OLD_FASHIONED_BUILD_FALSE@clean-local:
@CONFIG_RTAI_OLD_FASHIONED_BUILD_FALSE@	rm -f .cfchanged

@CONFIG_RTAI_OLD_FASHIONED_BUILD_FALSE@distclean-local:
@CONFIG_RTAI_OLD_FASHIONED_BUILD_FALSE@	test -d $(top_builddir)/base/config && \
@CONFIG_RTAI_OLD_FASHIONED_BUILD_FALSE@	$(MAKE) -C $(top_builddir)/base/config/kconfig \
@CONFIG_RTAI_OLD_FASHIONED_BUILD_FALSE@	-f @abs_srcdir@/base/config/kconfig/Makefile.kconfig distclean srctree=@abs_srcdir@

